{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "port": 9339,
            "localRoot": "${workspaceFolder}",
            "name": "Docker: Attach to Game",
            "remoteRoot": "/usr/src/app"
        },
        {
            "type": "node",
            "request": "attach",
            "port": 9229,
            "localRoot": "${workspaceFolder}",
            "name": "Docker: Attach to Lobby",
            "remoteRoot": "/usr/src/app"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Remote",
            "address": "127.0.0.1",
            "port": 9229,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/home/keyteki/keyteki-lobby"
        },
        {
            "name": "Launch (Lobby Server)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/index.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": ["--nolazy"],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": false
        },
        {
            "name": "Launch (Game Server)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/server/gamenode/index.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": ["--nolazy"],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": false
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null
        },

        {
            "name": "Debug Tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "env": {
                "JASMINE_CONFIG_PATH": "./jasmine.json"
            },
            "cwd": "${workspaceRoot}"
        },
        {
            "name": "Debug Current Spec",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "env": {
                "JASMINE_CONFIG_PATH": "./jasmine.json"
            },
            "args": ["${relativeFile}"],
            "cwd": "${workspaceRoot}"
        }
    ]
}
